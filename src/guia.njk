  <script>
    document.addEventListener('DOMContentLoaded', () => {
      function toggleContent(id) {
        const content = document.getElementById(id);
        const button = document.querySelector(`[aria-controls="${id}"]`);
        const parent = button.closest('.md-toggle-item');
        const isOpen = parent.classList.contains('is-active');
        if (isOpen) {
          parent.classList.remove('is-active');
          button.setAttribute('aria-expanded', 'false');
          content.setAttribute('hidden', '');
        } else {
          parent.classList.add('is-active');
          button.setAttribute('aria-expanded', 'true');
          content.removeAttribute('hidden');
        }
      }
      // Se seleccionan todos los botones con la clase "md-toggle-button"
      const accordionButtons = document.querySelectorAll('.md-toggle-button');
      accordionButtons.forEach(button => {
        button.addEventListener('click', () => {
          const contentId = button.getAttribute('aria-controls');
          toggleContent(contentId);
        });
      });
      // Agrega una clase al body al presionar Tab para mejorar la accesibilidad con teclado
      document.addEventListener('keydown', event => {
        if (event.key === 'Tab') {
          document.body.classList.add('theme-keyboard-accesible');
        }
      });
    });
  </script>