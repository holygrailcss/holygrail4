<h1>{{ title }}</h1>
<section>
  <section class=" ptr-120 pbr-120">
    <div class="grid-cols-2 ">
      <h2> Grid System</h2>
      <p>
        The grid system uses a series of containers, rows, and columns to layout and align content.
      </p>
    </div>
  </section>
  <div class="grid-cols-12 gap-16 grid-examples">
    <div>1</div>
    <div>2</div>
    <div>3</div>
    <div>4</div>
    <div>5</div>
    <div>6</div>
    <div>7</div>
    <div>8</div>
    <div>9</div>
    <div>10</div>
    <div>11</div>
    <div>12</div>
  </div>
  <section class=" ptr-120 pbr-120">
    <h2>Resoluciones</h2>
    <div class="grid-cols-4 ">
      <div class="h3 variable-item border-b-grey"> Tamaño de Pantalla</div>
      <div class="h3 variable-item border-b-grey"> Número de Columnas</div>
      <div class="h3 variable-item border-b-grey"> Tipo de Resolución</div>
      <div class="h3 variable-item border-b-grey"> Ancho de Columnado</div>
      {% for key, data in maps.breakpointscols.value %}
      <div class="variable-item border-b-grey">
        <strong>{{ key }}</strong>
      </div>
      <div class="variable-item border-b-grey">
        <span class="code-snippet">{{ loop.index }}</span>
      </div>
      <div class="variable-item border-b-grey">
        <span class="code-snippet">
          {% if key == 'xs' %}Mobile{% elif key == 'sm' %}Tablet{% elif key == 'md' %}Desktop{% elif key == 'lg' %}Large
          Desktop{% elif key == 'xl' %}Extra Large Desktop{% endif %}
        </span>
      </div>
      <div class="variable-item border-b-grey">
        <span class="code-snippet">{{ data }}</span>
      </div>
      {% endfor %}
    </div>
  </section>
  <section class="ptr-120 pbr-120">
    <h2>Variables</h2>
    <ul class="list-clear color-list">
      {% for key, data in colors %}
      {% if data.type == "value" and data.value.startsWith('#') or data.value.startsWith('rgba') %}
      <li class="variable-item">
        <div class="variable-box" style="background-color: {{ data.value }};">
        </div>
        <div class="variable-info">
          <div>{{ data.var_name }}</div>
          <span class="code-snippet">{{ data.value }}</span>
        </div>
      </li>
      {% elif data.type == "value" %}
      <li class="variable-item">
        <div class="variable-info">
          <div>{{ data.var_name }}</div>
          <span class="code-snippet">{{ data.value }} </span>
        </div>
      </li>
      {% endif %}
      {% endfor %}
    </ul>
  </section>
  <section class="ptr-120 pbr-120">
    <h2>Typography</h2>
    {% for key, data in maps.minheaders.value %}
    <div class="grid-cols-6">
      <div class="variable-item border-b-grey">{{ key }}</div>
      <div class="variable-item border-b-grey {{ key }}"> Aa </div>
      {% for item in data %}
      <div class="variable-item border-b-grey">{{ item }}</div>
      {% endfor %}
    </div>
    {% endfor %}
  </section>
  <section class="ptr-120 pbr-120">
    <div class="grid-cols-2 gap-16">
      <div>
        <h2>Headers Fixers</h2>
        {% for key, data in maps.headersFixers.value %}
        <div class="grid-cols-3">
          <div class="variable-item border-b-grey">{{ key }}</div>
          {% for item in data %}
          <div class="variable-item border-b-grey">{{ item }}</div>
          {% endfor %}
        </div>
        {% endfor %}
      </div>
      <div>
        <h2>Headers Fluids</h2>
        {% for key, data in maps.headersFluids.value %}
        <div class="grid-cols-4">
          <div class="variable-item border-b-grey">{{ key }}</div>
          {% for item in data %}
          <div class="variable-item border-b-grey">{{ item }}</div>
          {% endfor %}
        </div>
        {% endfor %}
      </div>
    </div>
  </section>
